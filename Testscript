-- Load Rayfield
local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()

-- Create ConsoleLib purely for output
local ConsoleLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/Gitgenya/L4KETAKERRSCRIPTS/refs/heads/main/Consolelib"))()
local console = ConsoleLib:Create()
console:Output("ConsoleLib loaded! üêê")

-- Services
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Default dash/up speeds
local teleportSpeed = 500
local dashSpeed = 500

-- Rayfield Window
local Window = Rayfield:CreateWindow({Name = "Delta Hub"})

-- Money Tab
local MoneyTab = Window:CreateTab("Money")
MoneyTab:CreateButton({
    Name = "üí∞ Money Inf",
    Callback = function()
        console:Output("Running money dupe...")
        loadstring(game:HttpGet("https://rawscripts.net/raw/UPD-Build-A-Plane-money-dupe-51267"))()
        console:Output("Money dupe executed ‚úÖ")
    end
})

-- Dash/Up Tab
local MovementTab = Window:CreateTab("Movement")

MovementTab:CreateButton({
    Name = "‚¨ÜÔ∏è Teleport Up",
    Callback = function()
        local char = LocalPlayer.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            char.HumanoidRootPart.CFrame = char.HumanoidRootPart.CFrame + Vector3.new(0, teleportSpeed, 0)
            console:Output("Teleported up by "..teleportSpeed)
        end
    end
})

MovementTab:CreateButton({
    Name = "‚û°Ô∏è Dash Forward",
    Callback = function()
        local char = LocalPlayer.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            local forward = char.HumanoidRootPart.CFrame.LookVector * dashSpeed
            char.HumanoidRootPart.CFrame = char.HumanoidRootPart.CFrame + forward
            console:Output("Dashed forward by "..dashSpeed)
        end
    end
})

-- Dash/Teleport Speed Inputs
MovementTab:CreateInput({
    Name = "Teleport Speed",
    PlaceholderText = tostring(teleportSpeed),
    Callback = function(value)
        teleportSpeed = tonumber(value) or teleportSpeed
        console:Output("Teleport speed set to "..teleportSpeed)
    end
})

MovementTab:CreateInput({
    Name = "Dash Speed",
    PlaceholderText = tostring(dashSpeed),
    Callback = function(value)
        dashSpeed = tonumber(value) or dashSpeed
        console:Output("Dash speed set to "..dashSpeed)
    end
})
