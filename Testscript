-- Load Rayfield and your ConsoleLib
local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/SiriusSoftwareLtd/Rayfield/main/source.lua'))()
local ConsoleLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/Gitgenya/L4KETAKERRSCRIPTS/refs/heads/main/Consolelib'))()

-- Create Rayfield window
local Window = Rayfield:CreateWindow({
    Name = "Rayfield + ConsoleLib test2",
    LoadingTitle = "Initializing...",
    LoadingIcon = "rbxassetid://4483345998"
})

-- Main tab
local MainTab = Window:CreateTab("Main", 4483362458)

-- Section for placeholders
local PlaceholderSection = MainTab:CreateSection("Placeholders")

-- Info button
PlaceholderSection:CreateButton({
    Name = "Print Info",
    Callback = function()
        ConsoleLib.info("Placeholder INFO message.")
    end
})

-- Warning button
PlaceholderSection:CreateButton({
    Name = "Print Warning",
    Callback = function()
        ConsoleLib.warn("Placeholder WARNING message.")
    end
})

-- Error button
PlaceholderSection:CreateButton({
    Name = "Print Error",
    Callback = function()
        ConsoleLib.error("Placeholder ERROR message.")
    end
})

-- Toggle ConsoleLib button (keeps the Rayfield button visible)
PlaceholderSection:CreateButton({
    Name = "Toggle Console Window",
    Callback = function()
        -- Open or close your draggable console
        ConsoleLib.toggle()
        -- Optional: print to verify
        ConsoleLib.info("Console toggled via Rayfield button âœ…")
    end
})
